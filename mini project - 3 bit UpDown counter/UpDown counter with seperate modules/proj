#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55b41b16ebe0 .scope module, "tb_up_down_counter" "tb_up_down_counter" 2 3;
 .timescale -9 -12;
v0x55b41b1861b0_0 .net "Q_down", 2 0, v0x55b41b134c00_0;  1 drivers
v0x55b41b1862a0_0 .net "Q_up", 2 0, v0x55b41b185ee0_0;  1 drivers
v0x55b41b186370_0 .var "clk", 0 0;
v0x55b41b186490_0 .var "enable_down", 0 0;
v0x55b41b186530_0 .var "enable_up", 0 0;
S_0x55b41b16ed70 .scope module, "uut_down" "down_counter" 2 17, 3 1 0, S_0x55b41b16ebe0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 3 "Q";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "enable";
v0x55b41b134c00_0 .var "Q", 2 0;
v0x55b41b134370_0 .net "clk", 0 0, v0x55b41b186370_0;  1 drivers
v0x55b41b185b90_0 .net "enable", 0 0, v0x55b41b186490_0;  1 drivers
E_0x55b41b16d910 .event posedge, v0x55b41b134370_0;
S_0x55b41b185cb0 .scope module, "uut_up" "up_counter" 2 11, 4 1 0, S_0x55b41b16ebe0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 3 "Q";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "enable";
v0x55b41b185ee0_0 .var "Q", 2 0;
v0x55b41b185fe0_0 .net "clk", 0 0, v0x55b41b186370_0;  alias, 1 drivers
v0x55b41b1860a0_0 .net "enable", 0 0, v0x55b41b186530_0;  1 drivers
    .scope S_0x55b41b185cb0;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55b41b185ee0_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x55b41b185cb0;
T_1 ;
    %wait E_0x55b41b16d910;
    %load/vec4 v0x55b41b1860a0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55b41b185ee0_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x55b41b185ee0_0, 0;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55b41b16ed70;
T_2 ;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x55b41b134c00_0, 0, 3;
    %end;
    .thread T_2;
    .scope S_0x55b41b16ed70;
T_3 ;
    %wait E_0x55b41b16d910;
    %load/vec4 v0x55b41b185b90_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x55b41b134c00_0;
    %subi 1, 0, 3;
    %assign/vec4 v0x55b41b134c00_0, 0;
T_3.0 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55b41b16ebe0;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b41b186370_0, 0, 1;
T_4.0 ;
    %delay 5000, 0;
    %load/vec4 v0x55b41b186370_0;
    %inv;
    %store/vec4 v0x55b41b186370_0, 0, 1;
    %jmp T_4.0;
    %end;
    .thread T_4;
    .scope S_0x55b41b16ebe0;
T_5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b41b186530_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b41b186490_0, 0, 1;
    %delay 80000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b41b186530_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b41b186490_0, 0, 1;
    %vpi_call 2 35 "$display", "Switching to Down Counter phase: " {0 0 0};
    %delay 80000, 0;
    %vpi_call 2 39 "$finish" {0 0 0};
    %end;
    .thread T_5;
    .scope S_0x55b41b16ebe0;
T_6 ;
T_6.0 ;
    %wait E_0x55b41b16d910;
    %load/vec4 v0x55b41b186530_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.1, 8;
    %vpi_call 2 46 "$display", "Up Counter phase: At time %t: clk = %b, Q_up = %b", $time, v0x55b41b186370_0, v0x55b41b1862a0_0 {0 0 0};
    %jmp T_6.2;
T_6.1 ;
    %load/vec4 v0x55b41b186490_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.3, 8;
    %vpi_call 2 48 "$display", "Down Counter phase: At time %t: clk = %b, Q_down = %b", $time, v0x55b41b186370_0, v0x55b41b1861b0_0 {0 0 0};
T_6.3 ;
T_6.2 ;
    %jmp T_6.0;
    %end;
    .thread T_6;
    .scope S_0x55b41b16ebe0;
T_7 ;
    %vpi_call 2 54 "$dumpfile", "up_down_counter.vcd" {0 0 0};
    %vpi_call 2 55 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55b41b16ebe0 {0 0 0};
    %end;
    .thread T_7;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "Up_down_tb.v";
    "down.v";
    "up.v";
