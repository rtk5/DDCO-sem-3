#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55eefb85bd90 .scope module, "tb_up_down_counter" "tb_up_down_counter" 2 3;
 .timescale -9 -12;
v0x55eefb8a9b90_0 .net "Q", 2 0, v0x55eefb85d7f0_0;  1 drivers
v0x55eefb8a9c80_0 .var "clk", 0 0;
v0x55eefb8a9d50_0 .var "up_down", 0 0;
S_0x55eefb85bf20 .scope module, "uut" "up_down_counter" 2 13, 3 2 0, S_0x55eefb85bd90;
 .timescale 0 0;
    .port_info 0 /OUTPUT 3 "Q";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "up_down";
v0x55eefb85d7f0_0 .var "Q", 2 0;
v0x55eefb85dc00_0 .net "clk", 0 0, v0x55eefb8a9c80_0;  1 drivers
v0x55eefb8a9a70_0 .net "up_down", 0 0, v0x55eefb8a9d50_0;  1 drivers
E_0x55eefb896c30 .event posedge, v0x55eefb85dc00_0;
    .scope S_0x55eefb85bf20;
T_0 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x55eefb85d7f0_0, 0, 3;
    %end;
    .thread T_0;
    .scope S_0x55eefb85bf20;
T_1 ;
    %wait E_0x55eefb896c30;
    %load/vec4 v0x55eefb8a9a70_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x55eefb85d7f0_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x55eefb85d7f0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55eefb85d7f0_0;
    %subi 1, 0, 3;
    %assign/vec4 v0x55eefb85d7f0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55eefb85bd90;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eefb8a9c80_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x55eefb8a9c80_0;
    %inv;
    %store/vec4 v0x55eefb8a9c80_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x55eefb85bd90;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55eefb8a9d50_0, 0, 1;
T_3.0 ;
    %wait E_0x55eefb896c30;
    %vpi_call 2 33 "$display", "At time %t: clk = %b, up_down = %b, Q = %b", $time, v0x55eefb8a9c80_0, v0x55eefb8a9d50_0, v0x55eefb8a9b90_0 {0 0 0};
    %jmp T_3.0;
    %end;
    .thread T_3;
    .scope S_0x55eefb85bd90;
T_4 ;
    %delay 80000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55eefb8a9d50_0, 0, 1;
    %delay 80000, 0;
    %vpi_call 2 49 "$finish" {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x55eefb85bd90;
T_5 ;
    %vpi_call 2 54 "$dumpfile", "up_down_counter.vcd" {0 0 0};
    %vpi_call 2 55 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55eefb85bd90 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "UpDown_tb.v";
    "UpDown.v";
